package practice1;

import java.util.*;

public class Practice01 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        String s = String.valueOf(n);
        System.out.println(s);
        if(n % 2 == 0){
            System.out.println(s + "is even");
        }else{
            System.out.println(s + "is odd");
        }
    }
}
	