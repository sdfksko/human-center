package practice1;

import java.util.*;

public class Practice01 {
	public static void main(String[] args) {
		System.out.println(solution("나비", 5));
	}
	 public static String solution(String my_string, int k) {
		 String answer = "";
		 for(int i = 1; i == k; i++) {
			 answer += my_string;
		 }
	        return answer;
	    }
	        
	    
}
