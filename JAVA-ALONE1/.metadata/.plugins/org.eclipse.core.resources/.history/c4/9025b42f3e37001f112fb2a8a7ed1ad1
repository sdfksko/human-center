package practice1;

import java.util.*;

public class Practice01 {
	public static void main(String[] args) {
		System.out.println(solution("나비", 5));
	}
	 public static String solution(String my_string, int k) {
		 String answer = "";
		 answer = my_String * k;
	        return answer;
	    }
	        
	    
}
